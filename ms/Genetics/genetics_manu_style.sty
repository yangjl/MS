                     {-.55em \@plus -.2em}%                                     {\normalfont\normalsize\itshape}}\setcounter{secnumdepth}{0}%% FORMAT THE TITLE PAGE\renewcommand{\thefootnote}{\fnsymbol{footnote}}\renewcommand\maketitle{%\begin{titlepage}%  \let\TempFootNoteSize\footnotesize  \let\footnotesize\normalsize  \let\footnoterule\relax  \let \footnote \thanks  \addtolength{\footnotesep}{1.66\footnotesep}  \null\vfil  \vskip 60\p@  \begin{center}%    {\Large \@title \par}%    \vskip 3em%    {\normalsize     \lineskip .75em%      \begin{tabular}[t]{c}%        \@author      \end{tabular}\par}%      \vskip 1.5em%    {\normalsize \@date \par}%       % Set date in \large size.  \end{center}\par  \@thanks  \vfil\null%  \end{titlepage}%  \setcounter{footnote}{0}%  \global\let\thanks\relax  \global\let\maketitle\relax  \global\let\@thanks\@empty  \global\let\@author\@empty  \global\let\@date\@empty  \global\let\@title\@empty  \global\let\title\relax  \global\let\author\relax  \global\let\date\relax  \global\let\and\relax	 \global\let\footnotesize\TempFootNoteSize  %    BELOW HERE IT PRINTS PAGE 2.  \clearpage  \vspace*{15ex}  \begin{center}     Running Head: \RunningHead  \end{center}     \vskip 20\p@	   \newbox{\KeyWordstempboxa}    \savebox{\KeyWordstempboxa}{KeyWords: \KeyWords}%    \ifdim \wd\KeyWordstempboxa >\textwidth      Key Words: \KeyWords    \else     \begin{center}        Key Words:  \KeyWords      \end{center}    \fi     \vskip 25\p@     \begin{center}      Corresponding Author:      \vskip 12\p@      \CorrespondingAuthor      \vskip 30\p@      \vfil      \CorrespondingAddress     \end{center}     \vfil}%%  Format the abstract page appropriately\renewenvironment{abstract}{%      \clearpage      \null\vfil      \@beginparpenalty\@lowpenalty      \begin{center}%        \bfseries \abstractname        \@endparpenalty\@M      \end{center}}%      {\par\vfil\null\clearpage}    