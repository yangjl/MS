	Mesh = {Amino Acid Sequence; Amino Acid Substitution; Humans; Models, Molecular; Polymorphism, Single Nucleotide; Protein Conformation; Proteins},
	Pages = {61-80},
	Pmid = {16824020},
	Pst = {ppublish},
	Title = {Predicting the effects of amino acid substitutions on protein function},
